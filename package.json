{
  "name": "adonis-lucid-filter",
  "version": "5.0.0",
  "description": "Addon for filtering AdonisJS Lucid ORM",
  "engines": {
    "node": ">=18.6.0"
  },
  "main": "./build/index.js",
  "type": "module",
  "files": [
    "build"
  ],
  "exports": {
    ".": "./build/index.js",
    "./lucid_filter_provider": "./build/providers/lucid_filter_provider.js",
    "./types/*": "./build/src/types/*.js"
  },
  "scripts": {
    "lint": "eslint . --ext=.ts",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "clean": "rm -rf build",
    "compile": "npm run lint && npm run clean && tsc && npm run copy:files",
    "copy:files": "copyfiles \"templates/*.txt\" build",
    "build": "npm run compile",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "adonisjs",
    "adonisjs-filter",
    "adonisjs-lucid",
    "adonisjs-lucid-filter",
    "adonis",
    "adonis-lucid",
    "adonis-filter",
    "adonis-lucid-filter"
  ],
  "author": "LookinGit",
  "license": "MIT",
  "homepage": "https://github.com/lookinlab/adonis-lucid-filter#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lookinlab/adonis-lucid-filter.git"
  },
  "bugs": {
    "url": "https://github.com/lookinlab/adonis-lucid-filter/issues"
  },
  "dependencies": {
    "type-fest": "^2.19.0"
  },
  "devDependencies": {
    "@adonisjs/core": "^6.2.2",
    "@adonisjs/eslint-config": "^1.2.1",
    "@adonisjs/lucid": "^20.1.0",
    "@adonisjs/tsconfig": "^1.2.1",
    "@types/lodash": "^4.14.202",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.2.1",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "directories": {
    "test": "test"
  },
  "peerDependencies": {
    "@adonisjs/core": "^6.2.2",
    "@adonisjs/lucid": "^20.1.0",
    "lodash": "^4.17.21"
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  }
}
